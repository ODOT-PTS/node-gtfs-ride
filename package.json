{
  "name": "gtfs-ride",
  "description": "Build GTFS-Ride data from Automated Passenger Counter (APC) data.",
  "version": "0.1.0",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "gtfs-ride"
  ],
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node ./app",
    "test": "xo"
  },
  "bin": {
    "gtfs-ride": "bin/gtfs-ride.js"
  },
  "main": "index",
  "dependencies": {
    "chalk": "^4.1.1",
    "cli-table": "^0.3.6",
    "csv-parse": "^4.15.4",
    "express": "^4.17.1",
    "gtfs": "3.0.1",
    "lodash-es": "^4.17.21",
    "luxon": "^1.27.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "open": "^8.1.0",
    "pretty-error": "^3.0.3",
    "progress": "^2.0.3",
    "pug": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "strip-bom-stream": "^4.0.0",
    "timer-machine": "^1.1.0",
    "tmp-promise": "^3.0.2",
    "toposort": "^2.0.2",
    "untildify": "^4.0.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "xo": "^0.40.1"
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "false": {}
}
