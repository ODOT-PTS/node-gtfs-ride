{
  "name": "gtfs-ride",
  "description": "Build GTFS-Ride data from Automated Passenger Counter (APC) data.",
  "version": "0.2.1",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "gtfs-ride"
  ],
  "bugs": {
    "url": "https://github.com/ODOT-PTS/node-gtfs-ride/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ODOT-PTS/node-gtfs-ride.git"
  },
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "eslint **/*.js",
    "lint-fix": "eslint **/*.js --fix",
    "prepare": "husky install"
  },
  "bin": {
    "gtfs-ride": "bin/gtfs-ride.js"
  },
  "main": "index",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table": "^0.3.11",
    "csv-parse": "^5.5.5",
    "gtfs": "4.7.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.4",
    "node-fetch": "^3.3.2",
    "open": "^10.0.4",
    "pretty-error": "^4.0.0",
    "progress": "^2.0.3",
    "pug": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "strip-bom-stream": "^5.0.0",
    "timer-machine": "^1.1.0",
    "tmp-promise": "^3.0.3",
    "toposort": "^2.0.2",
    "untildify": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">= 16.15.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": "prettier --write",
    "*.json": "prettier --write"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  }
}
