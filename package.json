{
  "name": "gtfs-ride",
  "description": "Build GTFS-Ride data from Automated Passenger Counter (APC) data.",
  "version": "0.1.4",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "gtfs-ride"
  ],
  "bugs": {
    "url": "https://github.com/ODOT-PTS/node-gtfs-ride/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ODOT-PTS/node-gtfs-ride.git"
  },
  "private": false,
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node ./app",
    "lint": "xo --fix"
  },
  "bin": {
    "gtfs-ride": "bin/gtfs-ride.js"
  },
  "main": "index",
  "dependencies": {
    "chalk": "^5.0.1",
    "cli-table": "^0.3.11",
    "csv-parse": "^5.0.4",
    "gtfs": "3.3.1",
    "lodash-es": "^4.17.21",
    "luxon": "^2.3.2",
    "node-fetch": "^3.2.4",
    "open": "^8.4.0",
    "pretty-error": "^4.0.0",
    "progress": "^2.0.3",
    "pug": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "strip-bom-stream": "^5.0.0",
    "timer-machine": "^1.1.0",
    "tmp-promise": "^3.0.3",
    "toposort": "^2.0.2",
    "untildify": "^4.0.0",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "xo": "^0.48.0"
  },
  "engines": {
    "node": ">= 16.13.1"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  }
}
